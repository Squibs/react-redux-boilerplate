{
  "name": "react-redux-boilerplate",
  "private": true,
  "version": "1.0.0",
  "description": "A personal starting point for my own React-Redux projects.",
  "author": "Zachary Holman",
  "license": "ISC",
  "scripts": {
    "build": "cross-env NODE_ENV=production node scripts/build.js",
    "start": "cross-env NODE_ENV=development node --trace-deprecation scripts/start.js",
    "webpack": "cross-env NODE_ENV=production webpack --config config/webpack.config.js"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.2",
    "@babel/node": "^7.0.0-rc.2",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-rc.2",
    "@babel/polyfill": "^7.0.0-rc.2",
    "@babel/preset-env": "^7.0.0-rc.2",
    "@babel/preset-react": "^7.0.0-rc.2",
    "babel-eslint": "^9.0.0-beta.3",
    "babel-loader": "^8.0.0-beta.6",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "cors": "^2.8.4",
    "cross-env": "^5.2.0",
    "dotenv": "^6.0.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "express": "^4.16.3",
    "express-manifest-helpers": "^0.5.0",
    "file-loader": "^2.0.0",
    "img-loader": "^3.0.0",
    "nodemon": "^1.18.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-hot-loader": "^4.3.5",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "rimraf": "^2.6.2",
    "url-loader": "^1.1.1",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.2.0",
    "webpack-hot-middleware": "^2.22.3",
    "webpack-manifest-plugin": "^2.0.3",
    "webpack-node-externals": "^1.7.2",
    "write-file-webpack-plugin": "^4.3.2"
  },
  "dependencies": {},
  "browserslist": [
    "last 2 versions"
  ],
  "eslintConfig": {
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "browser": true,
      "es6": true
    },
    "rules": {
      "func-names": [
        "error",
        "never"
      ],
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": true
        }
      ]
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import"
    ]
  }
}
