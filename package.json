{
  "name": "react-redux-boilerplate",
  "private": true,
  "version": "1.0.0",
  "description": "A personal starting point for my own React-Redux projects.",
  "scripts": {
    "build": "cross-env NODE_ENV=production node scripts/build.js",
    "start": "cross-env NODE_ENV=development node --trace-deprecation scripts/start.js",
    "webpack": "cross-env NODE_ENV=production webpack --config config/webpack.config.js"
  },
  "keywords": [],
  "author": "Zachary Holman",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.46",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.46",
    "@babel/polyfill": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/preset-react": "^7.0.0-beta.46",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^8.0.0-beta.2",
    "body-parser": "^1.18.2",
    "chalk": "^2.4.1",
    "cors": "^2.8.4",
    "cross-env": "^5.1.4",
    "dotenv": "^5.0.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "express": "^4.16.3",
    "express-manifest-helpers": "^0.5.0",
    "file-loader": "^1.1.11",
    "img-loader": "^2.0.1",
    "nodemon": "^1.17.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-hot-loader": "^4.1.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.2",
    "url-loader": "^1.0.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.2",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.1",
    "webpack-manifest-plugin": "^2.0.2",
    "webpack-node-externals": "^1.7.2",
    "write-file-webpack-plugin": "^4.2.0"
  },
  "dependencies": {},
  "browserslist": [
    "last 2 versions"
  ],
  "eslintConfig": {
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "browser": true,
      "es6": true
    },
    "rules": {
      "func-names": [
        "error",
        "never"
      ]
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import"
    ]
  }
}
