{
  "name": "react-redux-boilerplate",
  "private": true,
  "version": "1.0.0",
  "description": "A personal starting point for my own React-Redux projects.",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "Zachary Holman",
  "license": "ISC",
  "devDependencies": {},
  "dependencies": {},
  "browserslist": [
    "last 2 versions"
  ],
  "eslintConfig": {
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "browser": true,
      "es6": true
    },
    "rules": {
      "func-names": [
        "error",
        "never"
      ]
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import"
    ],
    "env": {
      "development": {
        "plugins": [
          "react-hot-loader/babel"
        ]
      }
    }
  }
}
